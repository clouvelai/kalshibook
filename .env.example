# Kalshi API
KALSHI_API_KEY_ID=your-kalshi-api-key-id
# Provide EITHER key content (inline) OR key path (file) - not both
KALSHI_PRIVATE_KEY_CONTENT="-----BEGIN RSA PRIVATE KEY-----\n...\n-----END RSA PRIVATE KEY-----"
# KALSHI_PRIVATE_KEY_PATH=./kalshi_private_key.pem

# Supabase
SUPABASE_URL=http://127.0.0.1:54321
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
DATABASE_URL=postgresql://postgres:postgres@127.0.0.1:54322/postgres

# App
APP_ENV=development

# API Server
API_HOST=0.0.0.0
API_PORT=8000
API_RATE_LIMIT_DEFAULT=100

# Stripe
STRIPE_SECRET_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...
STRIPE_PROJECT_PRICE_ID=price_...
STRIPE_METER_EVENT_NAME=kalshibook_api_credits
STRIPE_METER_EVENT_ID=mtr_...

# Google OAuth (for dashboard login)
GOOGLE_OAUTH_CLIENT_ID=your-google-oauth-client-id
GOOGLE_OAUTH_CLIENT_SECRET=your-google-oauth-client-secret
